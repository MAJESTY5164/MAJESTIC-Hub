})

  Tab:AddButton({
    Name = "Aimbot - Made By neural",
    Callback = function()
        -- when you press your left mouse button the aimbot is enabled, when u let it go it gets disabled.

getgenv().Settings = {
Radius = 100, -- The size of your aimbot FOV, i recommend between 50 - 300 :)
